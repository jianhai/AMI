postinstall scriptlet (using /bin/sh):
if [ -f /usr/share/info/gawk.info.gz ]; then
    /sbin/install-info /usr/share/info/gawk.info.gz /usr/share/info/dir || :
fi
preuninstall scriptlet (using /bin/sh):
if [ $1 = 0 -a -f /usr/share/info/gawk.info.gz ]; then
    /sbin/install-info --delete /usr/share/info/gawk.info.gz /usr/share/info/dir || :
fi
