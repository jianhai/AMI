postinstall scriptlet (using /bin/sh):
/sbin/ldconfig
/sbin/install-info /usr/share/info/libcdio.info /usr/share/info/dir 2>/dev/null || :
preuninstall scriptlet (using /bin/sh):
if [ $1 = 0 ]; then
	/sbin/install-info --delete /usr/share/info/libcdio.info \
		/usr/share/info/dir 2>/dev/null || :
fi
postuninstall program: /sbin/ldconfig
